{
  "name": "yagodgo/webapp",
  "description": "Веб-приложение доставки ягод и фруктов ЯгодGO",
  "type": "project",
  "require": {
    "php": ">=8.0",
    "bramus/router": "^1.6",
    "vlucas/phpdotenv": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "start": "php -S localhost:8000 -t ."
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}